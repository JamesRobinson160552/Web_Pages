html{
	background-color:blue;
}
body{
	background:black;
	color:white;
	font-family:helvetica;
	margin:2%;
	padding:5%;
	border:5px solid white;
}
h1, h2{
	text-align:center;
	margin:5%;
}
h1{
	font-size:500%;
}
p{
	font-size:200%;
}
img{
	display:block;
	margin-left:auto;
	margin-right:auto;
	margin-top:5%;
	margin-bottom:5%;
}
.navBar{
	border: solid 3px blue;
	color:blue;
	background: white;
	font-size: 200%;
	margin: 1%;
	padding:1%;
	text-align:center;
}
a{
	text-decoration:none;
	padding-right:10%;
	padding-left:10%;
}
ol{
	border:solid blue 5px;
}
li{
	font-size:200%;
	margin:2%;
}
.caption{
	font-size:100%;
	text-align:center;
}
table{
	border:solid blue 5px;
	border-collapse:collapse;
}
th, td{
	text-align:left;
	border:solid white 2px;
	border-collapse:collapse;
	font-size:300%;
	padding:12px;
}
footer{
	font-size:100%;
	display:block;
	font-stretch:expanded;
	padding:10px;
	color:white;
	background-color:blue;
	text-align:center;
}